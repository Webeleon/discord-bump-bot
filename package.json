{
  "name": "bump-bot",
  "version": "0.0.2",
  "description": "",
  "contributors": [
    {
      "name": "Julien Prugne",
      "email": "julien@webeleon.dev"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Webeleon/discord-bump-bot.git"
  },
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --detectOpenHandles --force-exit",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage --detectOpenHandles --force-exit",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --force-exit"
  },
  "dependencies": {
    "@nestjs/common": "7.6.11",
    "@nestjs/core": "7.6.11",
    "@nestjs/mongoose": "7.2.3",
    "@nestjs/platform-express": "7.6.11",
    "@nestjs/schedule": "0.4.2",
    "dblapi.js": "2.4.1",
    "discord.js": "12.5.1",
    "dotenv": "8.2.0",
    "mongoose": "5.11.15",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "6.6.3"
  },
  "devDependencies": {
    "@nestjs/cli": "7.5.4",
    "@nestjs/schematics": "7.2.8",
    "@nestjs/testing": "7.6.11",
    "@types/express": "4.17.11",
    "@types/jest": "26.0.20",
    "@types/mongoose": "5.10.3",
    "@types/node": "13.13.45",
    "@types/supertest": "2.0.10",
    "@typescript-eslint/eslint-plugin": "4.14.2",
    "@typescript-eslint/parser": "4.14.2",
    "eslint": "7.21.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-import": "2.22.1",
    "jest": "26.6.3",
    "prettier": "2.2.1",
    "supertest": "6.1.3",
    "ts-jest": "26.5.3",
    "ts-loader": "8.0.15",
    "ts-node": "9.1.1",
    "tsconfig-paths": "3.9.0",
    "typescript": "4.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
